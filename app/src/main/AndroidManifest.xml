<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.altrovis.jakhub">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".ActivityMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityHome" />
        <activity
            android:name=".ActivityKK"
            android:parentActivityName=".ActivityHome"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ActivityPerpanjangKTP"
            android:parentActivityName=".ActivityHome"
            android:theme="@style/AppTheme" />
        <activity android:name=".ActivityDetailNotifikasi"></activity>
    </application>

</manifest>
